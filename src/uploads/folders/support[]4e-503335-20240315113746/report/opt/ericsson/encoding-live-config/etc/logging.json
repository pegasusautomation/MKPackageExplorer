rs": {
        "none": {
            "class": "logging.NullHandler"
        },
        "standard_output": {
            "class": "logging.StreamHandler",
            "formatter": "json",
            "level": "INFO",
            "stream": "ext://sys.stdout"
        },
        "default_rotating_file": {
            "level": "INFO",
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "verbose",
            "filename": "/var/log/ericsson/encoding-live-config/private/config.log",
            "maxBytes": 5242880,
            "backupCount": 2,
            "encoding": "utf8"
        },
        "syslog": {
            "level": "INFO",
            "class": "logging.handlers.SysLogHandler",
            "facility": "local0",
            "formatter": "cee",
            "address": "/dev/log"
        }
    },
    "root": {
        "handlers": [
            "syslog",
            "default_rotating_file",
            "standard_output"
        ],
        "level": "INFO"
    },
    "loggers": {
        "envivio.liveencoder.config.models": {
            "handlers": [
                "syslog",
                "default_rotating_file",
                "standard_output"
            ],
            "propagate": false,
            "level": "WARNING"
        },
        "tornado.access": {
            "handlers": [
                "syslog",
                "default_rotating_file",
                "standard_output"
            ],
            "propagate": false,
            "level": "INFO",
            "filters": ["probes"]
        },
        "tornado": {
            "handlers": [
                "syslog",
                "default_rotating_file",
                "standard_output"
            ],
            "propagate": false,
            "level": "WARNING"
        },
        "pika": {
            "handlers": [
                "syslog",
                "default_rotating_file",
                "standard_output"
            ],
            "propagate": false,
            "level": "WARNING"
        }
    }
}{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "verbose": {
            "()": "envivio.liveencoder.config.logger.MicroServiceFormatter",
            "format": "%(hostname)s %(asctime)s [%(levelname)s] %(name)s %(module)s %(process)d %(threadName)s %(message)s"
        },
        "json": {
            "()": "envivio.liveencoder.config.logger.ExtendedJsonFormatter"
        },
        "cee": {
            "()": "envivio.liveencoder.config.logger.CeeFormatter"
        }
    },
    "filters": {
      "probes": {
        "()": "envivio.liveencoder.config.logger.ProbesFilter"
      }
    },
    "handle